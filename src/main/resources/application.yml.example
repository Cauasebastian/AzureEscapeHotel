server:
  port: 9192

spring:
  datasource:
    username: your_username
    password: your_password
    url: jdbc:mysql://your_database_url:3306/your_database_name
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    show-sql: true
    properties:
      hibernate:
        format_sql: true
    hibernate:
      ddl-auto: update
  servlet:
    multipart:
      max-file-size: 5MB
      max-request-size: 5MB
      file-size-threshold: 2KB
